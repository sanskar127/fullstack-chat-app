{
  "version": 2,
  "builds": [
    {
      "src": "src/socket/socket.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/socket",
      "dest": "/src/socket/socket.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/client/$1"
    }
  ]
}
